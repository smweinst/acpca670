\name{sim_dat_fun}
\alias{sim_dat_fun}
\title{Simulate data from the AC-PCA paper}
\usage{
sim_dat_fun(n = 5, b = 10, p = 400, alpha = 2.5)
}
\arguments{
\item{n}{the number of subjects; default is 5}

\item{b}{the number of brain regions; default is 10}

\item{p}{the number of features per brain region; default is 400}

\item{alpha}{the constant that is multiplied by the confounder; default is 2.5}
}
\description{
Simulate data from the AC-PCA paper
}
